<h1> Add New Picture </h1>

<%= form_for (@picture) do |f| %>

<%= f.label :name %>
<p> <%= f.text_field :name %> </p>

<%= f.label :attachment %>
<p> <%= f.file_field :attachment %> </p>
<br>
<%= f.submit "Upload Image" %>
<% end %>
<br>
<p> <%= link_to "User Index", users_path %> </p>
<p> <%= link_to "Picture Index", pictures_path %> </p>
