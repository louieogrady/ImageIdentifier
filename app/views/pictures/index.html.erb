<h1> Pictures Index </h1>

<div class="imagescontainer">
  <% @pictures.each do |picture| %>
  <div>
    <p> <%= link_to image_tag("#{picture.attachment}", height: 300 ), user_picture_path(@user, picture) %> </p>
  </div>
  <% end %>
</div>

<br>
<p> <%= link_to "Upload New Picture", new_user_picture_path %> </p>
<br>
<p> <%= link_to "User Index", users_path %> </p>
